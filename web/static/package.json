{
	"devDependencies": {
		"sass": "^1.69.5",
		"typedoc-plugin-merge-modules": "^5.1.0",
		"typescript": "^5.2.2"
	},
	"scripts": {
		"watch-css": "npx sass -w scss/main.scss style.css --embed-sources --style compressed",
		"watch-ts": "npx tsc -w",
		"documentation": "npx typedoc src/** --exclude src/lib/** --plugin typedoc-plugin-merge-modules"
	}
}
